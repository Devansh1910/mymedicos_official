<?xml version="1.0" encoding="utf-8"?>
<androidx.swiperefreshlayout.widget.SwipeRefreshLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/backgroundcolor"
    android:layout_height="match_parent"
    android:id="@+id/swipeRefreshLayout"
    tools:context=".activities.home.fragments.HomeFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:background="@color/backgroundcolor"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/progressBar"
            android:elevation="@dimen/_10sdp"
            android:visibility="gone"
            android:background="@drawable/back6"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <com.airbnb.lottie.LottieAnimationView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:elevation="10dp"
                android:padding="@dimen/_10sdp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:lottie_autoPlay="true"
                app:lottie_rawRes="@raw/shimmer"
                app:lottie_loop="false"
                app:lottie_speed="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ScrollView
                android:id="@+id/scrollerforthehome"
                android:layout_width="match_parent"
                android:background="@color/blue"
                android:layout_height="wrap_content">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentStart="true"
                        android:scaleType="centerCrop"
                        android:layout_alignParentEnd="true"
                        android:src="@drawable/vector_toppp" />

                    <androidx.cardview.widget.CardView
                        android:id="@+id/cardviewfull"
                        android:layout_marginTop="@dimen/_170sdp"
                        android:layout_width="match_parent"
                        app:cardCornerRadius="@dimen/_10ssp"
                        android:outlineAmbientShadowColor="@color/black"
                        android:elevation="@dimen/_10ssp"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:paddingVertical="@dimen/_5sdp"
                                android:elevation="2dp"
                                android:paddingHorizontal="@dimen/_5sdp"
                                android:orientation="vertical">

                                <androidx.cardview.widget.CardView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginHorizontal="@dimen/_5ssp"
                                    app:cardCornerRadius="@dimen/_10ssp">

                                    <androidx.cardview.widget.CardView
                                        android:layout_width="match_parent"
                                        android:paddingVertical="4dp"
                                        android:layout_height="wrap_content"
                                        app:cardCornerRadius="8dp">

                                        <org.imaginativeworld.whynotimagecarousel.ImageCarousel
                                            android:id="@+id/homecarousel"
                                            android:layout_width="match_parent"
                                            android:layout_height="100dp"
                                            android:elevation="@dimen/_10ssp"
                                            app:autoPlay="true"
                                            app:showTopShadow="false"
                                            app:showIndicator="false"
                                            app:showBottomShadow="false"
                                            app:showCaption="false"
                                            app:showNavigationButtons="false" />

                                    </androidx.cardview.widget.CardView>


                                </androidx.cardview.widget.CardView>

                            </LinearLayout>

                            <!-- Explore -->

                            <TextView
                                android:layout_width="match_parent"
                                android:paddingHorizontal="@dimen/_10ssp"
                                android:layout_height="match_parent"
                                android:layout_gravity="center_vertical"
                                android:gravity="center_vertical"
                                android:text="Features"
                                android:fontFamily="@font/inter"
                                android:textSize="@dimen/_12ssp"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_8ssp"
                                android:text="Explore the features!"
                                android:paddingHorizontal="@dimen/_10sdp"
                                android:fontFamily="@font/inter"
                                android:textColor="#919392"
                                android:textSize="@dimen/_10ssp"
                                android:textStyle="bold" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_gravity="center"
                                    android:gravity="center"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/cme_img1"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:scaleType="centerCrop"
                                        android:elevation="@dimen/_10ssp"
                                        android:src="@drawable/cnen" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:text="CME"
                                        android:textSize="@dimen/_10sdp"
                                        tools:ignore="TextSizeCheck" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_weight="1"                                android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/pub_image"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:scaleType="centerCrop"
                                        android:elevation="@dimen/_10ssp"
                                        android:src="@drawable/lib_new" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:text="Library"
                                        android:textAlignment="center"
                                        android:textSize="@dimen/_10ssp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_weight="1"                                android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/news_home"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:scaleType="centerCrop"
                                        android:elevation="@dimen/_10ssp"
                                        android:src="@drawable/news_new" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:text="News"
                                        android:textAlignment="center"
                                        android:textSize="@dimen/_10ssp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_weight="1"                                android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/jobs_img"
                                        android:scaleType="centerCrop"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:elevation="@dimen/_10ssp"
                                        android:src="@drawable/job_new" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:text="Jobs"
                                        android:textSize="@dimen/_10ssp" />
                                </LinearLayout>

                            </LinearLayout>

                            <TextView
                                android:layout_width="match_parent"
                                android:paddingHorizontal="@dimen/_10ssp"
                                android:layout_height="match_parent"
                                android:layout_gravity="center_vertical"
                                android:layout_marginTop="@dimen/_5ssp"
                                android:gravity="center_vertical"
                                android:text="Education"
                                android:fontFamily="@font/inter"
                                android:textSize="@dimen/_12ssp"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Enhancing Education Through Innovation"
                                android:paddingHorizontal="@dimen/_10sdp"
                                android:fontFamily="@font/inter"
                                android:textColor="#919392"
                                android:textSize="@dimen/_10ssp"
                                android:textStyle="bold" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:paddingTop="@dimen/_8ssp"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_weight="1"                                android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/ugexams"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:scaleType="centerCrop"
                                        android:elevation="@dimen/_10ssp"
                                        android:src="@drawable/ug_new" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:text="UG exams"
                                        android:textSize="@dimen/_10sdp"
                                        tools:ignore="TextSizeCheck" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_weight="1"                                android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/pg_prep"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:scaleType="centerCrop"
                                        android:elevation="@dimen/_10ssp"
                                        android:src="@drawable/pg_new" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:text="PG Neet"
                                        android:textAlignment="center"
                                        android:textSize="@dimen/_10ssp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/university"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:elevation="@dimen/_10ssp"
                                        android:scaleType="centerCrop"
                                        android:src="@drawable/cnen" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:text="FMGE"
                                        android:textAlignment="center"
                                        android:textSize="@dimen/_10ssp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/meme"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:scaleType="centerCrop"
                                        android:elevation="@dimen/_10ssp"
                                        android:src="@drawable/cnen" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:text="Neet ss"
                                        android:textSize="@dimen/_10ssp" />
                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:paddingHorizontal="@dimen/_10ssp"
                                android:layout_marginTop="@dimen/_10ssp"
                                android:elevation="@dimen/_16sdp"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center_vertical"
                                    android:gravity="center_vertical"
                                    android:text="Quick Apply"
                                    android:fontFamily="@font/inter"
                                    android:textSize="@dimen/_12ssp"
                                    android:textStyle="bold" />

                                <TextView
                                    android:id="@+id/navigatejobs"
                                    android:layout_width="wrap_content"
                                    android:fontFamily="@font/inter"
                                    android:layout_height="wrap_content"
                                    android:text="Explore"
                                    android:paddingHorizontal="@dimen/_8sdp"
                                    android:paddingVertical="@dimen/_2sdp"
                                    android:drawablePadding="@dimen/_8ssp"
                                    android:layout_gravity="center"
                                    android:gravity="end"
                                    android:textColor="@color/blue"
                                    android:textSize="@dimen/_10ssp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Latest Jobs Opening, Recommended for you!"
                                android:paddingHorizontal="@dimen/_10sdp"
                                android:layout_marginBottom="@dimen/_5ssp"
                                android:fontFamily="@font/inter"
                                android:textColor="#919392"
                                android:textSize="@dimen/_10ssp"
                                android:textStyle="bold" />

                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/recyclerview_job1"
                                android:layout_width="match_parent"
                                android:paddingVertical="@dimen/_5sdp"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/_5sdp"
                                android:layout_marginLeft="@dimen/_5sdp"
                                android:orientation="horizontal">

                            </androidx.recyclerview.widget.RecyclerView>

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardjobs"
                                app:cardCornerRadius="@dimen/_5sdp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="8dp"
                                android:elevation="4dp">

                                <!-- Card Content Layout -->
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    android:padding="16dp">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="Explore"
                                        android:fontFamily="@font/inter"
                                        android:textSize="16sp"
                                        android:drawableEnd="@drawable/arrowright"
                                        android:layout_gravity="center_horizontal"
                                        android:textColor="@android:color/black"/>

                                    <!-- Add more views or customize the layout as needed -->

                                </LinearLayout>

                            </androidx.cardview.widget.CardView>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:paddingHorizontal="@dimen/_10ssp"
                                android:elevation="@dimen/_16sdp"
                                android:layout_marginTop="@dimen/_10ssp"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center_vertical"
                                    android:gravity="center_vertical"
                                    android:text="Quick Explore"
                                    android:textSize="@dimen/_12ssp"
                                    android:fontFamily="@font/inter"
                                    android:textStyle="bold" />

                                <TextView
                                    android:id="@+id/navigatecmeinsider"
                                    android:layout_width="wrap_content"
                                    android:fontFamily="@font/inter"
                                    android:layout_height="wrap_content"
                                    android:text="View All"
                                    android:paddingHorizontal="@dimen/_8sdp"
                                    android:paddingVertical="@dimen/_2sdp"
                                    android:background="@drawable/categoryblackkkkkk"
                                    android:drawablePadding="@dimen/_8ssp"
                                    android:layout_gravity="center"
                                    android:gravity="end"
                                    android:textColor="@color/unselected"
                                    android:textSize="@dimen/_12ssp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_5ssp"
                                android:text="Checkout the latest CME in a Click!"
                                android:paddingHorizontal="@dimen/_10sdp"
                                android:fontFamily="@font/inter"
                                android:textColor="#919392"
                                android:textSize="@dimen/_10ssp"
                                android:textStyle="bold" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_5ssp"
                                android:paddingHorizontal="@dimen/_10ssp">

                                <TextView
                                    android:id="@+id/home1"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_5ssp"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/inter"
                                    android:background="@drawable/colorofhomeactivity"
                                    android:paddingHorizontal="@dimen/_5ssp"
                                    android:paddingVertical="@dimen/_10ssp"
                                    android:text="Upcoming"
                                    android:textAlignment="center"
                                    android:textColor="#454242"
                                    android:textSize="@dimen/_12ssp"
                                    android:textStyle="bold" />

                                <TextView
                                    android:id="@+id/home2"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_5ssp"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/inter"
                                    android:background="@drawable/colorofhomeactivity"
                                    android:paddingHorizontal="@dimen/_5ssp"
                                    android:paddingVertical="@dimen/_10ssp"
                                    android:text="Past"
                                    android:textAlignment="center"
                                    android:textColor="#454242"
                                    android:textSize="@dimen/_12ssp"
                                    android:textStyle="bold" />

                                <TextView
                                    android:id="@+id/home3"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/inter"
                                    android:background="@drawable/colorofhomeactivity"
                                    android:paddingHorizontal="@dimen/_5ssp"
                                    android:paddingVertical="@dimen/_10ssp"
                                    android:text="Ongoing"
                                    android:textAlignment="center"
                                    android:textColor="#454242"
                                    android:textSize="@dimen/_12ssp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_marginTop="@dimen/_5ssp"
                                android:paddingHorizontal="@dimen/_10ssp"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/inter"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center_vertical"
                                    android:gravity="center_vertical"
                                    android:text="Featured CME"
                                    android:textSize="@dimen/_12ssp"
                                    android:textStyle="bold" />

                                <TextView
                                    android:id="@+id/navigatecme"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="View All"
                                    android:paddingHorizontal="@dimen/_8sdp"
                                    android:paddingVertical="@dimen/_2sdp"
                                    android:background="@drawable/categoryblackkkkkk"
                                    android:fontFamily="@font/inter"
                                    android:drawablePadding="@dimen/_8ssp"
                                    android:layout_gravity="center"
                                    android:gravity="end"
                                    android:textColor="@color/unselected"
                                    android:textSize="@dimen/_12ssp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_5ssp"
                                android:text="Upscale your knowledge, Listen to the Professionals of your domain."
                                android:paddingHorizontal="@dimen/_10sdp"
                                android:fontFamily="@font/inter"
                                android:textColor="#919392"
                                android:textSize="@dimen/_10ssp"
                                android:textStyle="bold" />

                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/recyclerview_cme1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:layout_marginHorizontal="@dimen/_5sdp"
                                android:orientation="horizontal">

                            </androidx.recyclerview.widget.RecyclerView>

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardcme"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="8dp">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    android:padding="16dp">

                                    <TextView
                                        android:id="@+id/descriptionTextView"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="Explore"
                                        android:fontFamily="@font/inter"
                                        android:textSize="16sp"
                                        android:drawableEnd="@drawable/arrowright"
                                        android:layout_gravity="center_horizontal"
                                        android:textColor="@android:color/black"/>

                                </LinearLayout>

                            </androidx.cardview.widget.CardView>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:paddingHorizontal="@dimen/_10ssp"
                                android:elevation="@dimen/_16sdp"
                                android:layout_marginTop="@dimen/_10ssp"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:text="Recommended Slideshare"
                                    android:fontFamily="@font/inter"
                                    android:textSize="@dimen/_12ssp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/recyclerview_slideshare1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:paddingVertical="@dimen/_5ssp"
                                android:layout_marginBottom="@dimen/_5sdp"
                                android:layout_marginRight="@dimen/_5sdp"
                                android:layout_marginLeft="@dimen/_5sdp"
                                android:orientation="horizontal">

                            </androidx.recyclerview.widget.RecyclerView>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:paddingHorizontal="@dimen/_10ssp"
                                android:elevation="@dimen/_16sdp"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content"
                                    android:text="Live News"
                                    android:fontFamily="@font/inter"
                                    android:textSize="@dimen/_12ssp"
                                    android:textStyle="bold" />

                                <TextView
                                    android:id="@+id/navigatenews"
                                    android:layout_width="wrap_content"
                                    android:fontFamily="@font/inter"
                                    android:layout_height="wrap_content"
                                    android:text="View All"
                                    android:paddingHorizontal="@dimen/_8sdp"
                                    android:paddingVertical="@dimen/_2sdp"
                                    android:background="@drawable/categoryblackkkkkk"
                                    android:drawablePadding="@dimen/_8ssp"
                                    android:layout_gravity="center"
                                    android:gravity="end"
                                    android:textColor="@color/unselected"
                                    android:textSize="@dimen/_12ssp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/recyclerview_news"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:paddingVertical="@dimen/_10sdp"
                                android:layout_marginBottom="@dimen/_5sdp"
                                android:layout_marginRight="@dimen/_5sdp"
                                android:layout_marginLeft="@dimen/_5sdp"
                                android:orientation="horizontal">

                            </androidx.recyclerview.widget.RecyclerView>

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:scaleType="centerCrop"
                                android:layout_marginBottom="@dimen/_50ssp"
                                android:src="@drawable/connectfinal"/>

                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                </RelativeLayout>

            </ScrollView>
        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.swiperefreshlayout.widget.SwipeRefreshLayout>