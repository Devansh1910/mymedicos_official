<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".JobsActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.google.android.material.appbar.AppBarLayout
            android:id="@+id/cemtoolbar_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/jobstoolbar"
                app:title="Jobs"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:background="@color/white">

            </androidx.appcompat.widget.Toolbar>
        </com.google.android.material.appbar.AppBarLayout>

        <HorizontalScrollView
            android:layout_width="match_parent"
            android:scrollbars="none"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingVertical="@dimen/_10ssp"
                android:layout_marginBottom="@dimen/_5ssp"
                android:orientation="horizontal">

                <Spinner
                    android:id="@+id/city"
                    android:layout_width="match_parent"
                    android:paddingHorizontal="@dimen/_25ssp"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_marginHorizontal="@dimen/_2ssp"
                    android:layout_weight="1"
                    android:background="@drawable/spinner_new" />

                <Spinner
                    android:background="@drawable/spinner_new"
                    android:id="@+id/speciality"
                    android:paddingHorizontal="@dimen/_25ssp"
                    android:layout_width="match_parent"
                    android:layout_marginHorizontal="@dimen/_2ssp"
                    android:layout_weight="1"
                    android:layout_height="@dimen/_40sdp"/>

                <Spinner
                    android:background="@drawable/spinner_new"
                    android:id="@+id/subspeciality"
                    android:paddingHorizontal="@dimen/_25ssp"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:layout_marginHorizontal="@dimen/_5ssp"
                    android:layout_height="@dimen/_40sdp"/>
                <Button
                    android:layout_width="match_parent"
                    android:layout_height="52.00dp"
                    android:id="@+id/ok"
                    android:text="OK"
                    android:clickable="true"/>

            </LinearLayout>

        </HorizontalScrollView>

        <com.google.android.material.tabs.TabLayout
            android:id="@+id/jobtablayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <androidx.viewpager2.widget.ViewPager2
            android:id="@+id/jobViewpager"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tabLayout" />

