<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GetstartedActivity"
    android:background="@drawable/bgstarting">


    <TextView
        android:id="@+id/connecting_"
        android:layout_width="347dp"
        android:layout_height="98dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:textStyle="bold"
        android:gravity="top"
        android:text="by the medicos"
        android:textColor="#2c5733"
        android:textSize="44sp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="165dp" />


    <View
        android:id="@+id/rectangle_1"
        android:layout_width="382dp"
        android:layout_height="202dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/rectangle_14"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="441dp" />

    <TextView
        android:id="@+id/healing_wit"
        android:layout_width="219dp"
        android:layout_height="72dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:textStyle="bold"
        android:text="Healing with Heart\n Caring from Start "
        android:textColor="#23322D"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="160dp"
        tools:layout_editor_absoluteY="509dp" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/rectangle_3"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="695dp">


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="16sp"
            android:src="@drawable/Phone"
            tools:layout_editor_absoluteX="24dp"
            tools:layout_editor_absoluteY="586dp">


        </ImageView>

        <EditText

            android:layout_width="217dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginRight="30dp"
            android:layout_weight="1"
            android:inputType="text"
            android:text="Continue with Phone number" />

    </LinearLayout>




    <View
        android:id="@+id/rectangle_5"
        android:layout_width="400dp"
        android:layout_height="53.8dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/rectangle_5"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="871dp" />

    <TextView
        android:id="@+id/submit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:text="Submit "
        android:textColor="#23322D"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_5"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_5"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="@+id/rectangle_5"
        app:layout_constraintTop_toTopOf="@+id/rectangle_5" />


</androidx.constraintlayout.widget.ConstraintLayout>